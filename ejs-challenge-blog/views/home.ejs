<%-include("partials/header");-%>

<h1>Home</h1>
<p><%=homeContent%></p>
<% postInfo.forEach(function(post){%>
<h1><%=post.title %></h1>
<p>
  <%if (post.content.length>100){%>
  <%= post.content.slice(0,100)+" ..."%>
  <% }else{%>
  <%=post.content %>
  <% } %>
  <a href="posts/<%=post.title%>">Read More</a>

 </p>
<%}); %>



  <%-include("partials/footer");-%>
